<%= form_for [:employer , @member ] do |f| %>
        <div class="form-group">
            <li class="fa fa-user-circle" aria-hidden="true">
                <%= t("cp.full_name") %> :</li>
            <%= f.text_field :fullname ,placeholder: "", class: 'form-control' %>
        </div>
        <div class="form-group">
            <li class="fa fa-link" aria-hidden="true">
                <%= t("cp.link") %> :</li>
            <%= f.text_field :user_id ,placeholder: "", class: 'form-control' %>
        </div>
        <div class="submit-education">
            <div class="submit">
                <%= f.submit t("cp.submit"), class: "btn btn-primary"%>
            </div>
        </div>
        <% end %>
